[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/root/back-vits
ExecStart=/root/back-vits/venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/root/back-vits/server.sock server.wsgi:application

[Install]
WantedBy=multi-user.target